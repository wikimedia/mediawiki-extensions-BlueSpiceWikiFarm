			"services": [ "BlueSpiceWikiFarm._Config" ]
		},
		"addBootstrap": {
			"class": "BlueSpice\\WikiFarm\\Hook\\AddBootstrap"
		},
		"handleSharedResources": {
			"class": "BlueSpice\\WikiFarm\\Hook\\HandleSharedResources",
			"services": [
				"BlueSpiceWikiFarm._Config", "MainConfig", "TitleFactory",
				"BlueSpiceWikiFarm.GlobalDatabaseQuery", "BlueSpiceWikiFarm.InstanceStore",
				"BlueSpiceWikiFarm.AccessControlStore", "BlueSpiceWikiFarm.ForeignRequestExecution",
				"NamespaceInfo"
			]
		},
		"common-user-interface": {
			"class": "BlueSpice\\WikiFarm\\Hook\\CommonUserInterface",
			"services": [ "BlueSpiceWikiFarm.InstanceStore", "BlueSpiceWikiFarm._Config", "UserOptionsLookup" ]
		},
		"userpreference": {
			"class": "BlueSpice\\WikiFarm\\Hook\\AddUserPreference"
		}
	},
	"RestRoutes": [
		{
			"method": "POST",
			"path": "/bluespice/farm/v1/instance/{instanceName}",
			"class": "\\BlueSpice\\WikiFarm\\Rest\\CreateInstanceHandler",
			"services": [ "BlueSpiceWikiFarm.InstanceManager", "PermissionManager", "LanguageFactory", "MainConfig" ]
		},
		{
			"method": "POST",
			"path": "/bluespice/farm/v1/instance/edit/{instanceName}",
			"class": "\\BlueSpice\\WikiFarm\\Rest\\EditInstanceHandler",
			"services": [ "BlueSpiceWikiFarm.InstanceManager", "PermissionManager", "LanguageFactory", "MainConfig" ]
		},
		{
			"method": "POST",
			"path": "/bluespice/farm/v1/instance/clone/{sourceInstancePath}/{instanceName}",
			"class": "\\BlueSpice\\WikiFarm\\Rest\\CloneInstanceHandler",
			"services": [ "BlueSpiceWikiFarm.InstanceManager", "PermissionManager", "LanguageFactory", "MainConfig" ]
		},
		{
			"method": "POST",
			"path": "/bluespice/farm/v1/instance/suspend/{instanceName}",
			"class": "\\BlueSpice\\WikiFarm\\Rest\\SuspendHandler",
			"services": [ "BlueSpiceWikiFarm.InstanceManager", "PermissionManager" ]
		},
		{
			"method": "POST",
			"path": "/bluespice/farm/v1/instance/resume/{instanceName}",
			"class": "\\BlueSpice\\WikiFarm\\Rest\\ResumeHandler",
			"services": [ "BlueSpiceWikiFarm.InstanceManager", "PermissionManager", "BlueSpiceWikiFarm._InstanceCountLimiter" ]
		},
		{
			"method": "DELETE",
			"path": "/bluespice/farm/v1/instance/{instanceName}",
			"class": "\\BlueSpice\\WikiFarm\\Rest\\DeleteInstanceHandler",
			"services": [ "BlueSpiceWikiFarm.InstanceManager", "PermissionManager" ]
		},
		{
			"method": "GET",
			"path": "/bluespice/farm/v1/process/{processId}",
			"class": "\\BlueSpice\\WikiFarm\\Rest\\ProcessHandler",
			"services": [ "ProcessManager" ]
		},
		{
			"path": "/bluespice/farm/v1/limits",
			"class": "\\BlueSpice\\WikiFarm\\Rest\\LimitsHandler",
			"services": [ "PermissionManager", "BlueSpiceWikiFarm._InstanceCountLimiter" ]
		},
		{
			"method": "GET",
			"path": "/bluespice/farm/v1/generate_path",
			"class": "\\BlueSpice\\WikiFarm\\Rest\\GeneratePathHandler",
			"services": [ "BlueSpiceWikiFarm._InstancePathGenerator", "BlueSpiceWikiFarm.InstanceStore" ]
		},
		{
			"method": "GET",
			"path": "/bluespice/farm/v1/check_path_validity",
			"class": "\\BlueSpice\\WikiFarm\\Rest\\CheckPathHandler",
			"services": [ "BlueSpiceWikiFarm._InstancePathGenerator" ]
		},
		{
			"path": "/bluespice/farm/v1/combined-title-query-store",
			"class": "BlueSpice\\WikiFarm\\TitleSearch\\Rest\\FarmTitleQueryStore",
			"services": [
				"HookContainer",
				"DBLoadBalancer",
				"TitleFactory",
